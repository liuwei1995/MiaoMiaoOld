<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl_video_search_activity_main_include"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_below="@+id/ll"
    android:background="@drawable/baisebeijintupian"
    tools:context="com.zhaoyao.miaoyin.MainActivity" >

    <GridView
        android:id="@+id/gV_video_search_activity_main_include_prompt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawSelectorOnTop="true"
        android:horizontalSpacing="10dp"
        android:numColumns="2"
        android:scrollbars="none"
        android:stretchMode="columnWidth" />

</RelativeLayout>